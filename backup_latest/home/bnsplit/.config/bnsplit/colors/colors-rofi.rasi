    s-on-surface-variant: #F38E90;
    s-outline: #E71D21;
    s-outline-variant: #CE161A;
    s-shadow: #250405;
    s-scrim: #250405;
    s-inverse-surface: #FCE3E3;
    s-inverse-on-surface: #5D0A0C;
    s-inverse-primary: #B21316;
    s-surface-dim: #250405;
    s-surface-bright: #5D0A0C;
