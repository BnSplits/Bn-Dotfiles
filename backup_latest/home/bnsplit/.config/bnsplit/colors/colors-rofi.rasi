    s-primary: #A4977F;
    s-primary-fixed: #B1A691;
    s-primary-fixed-dim: #A4977F;
    s-on-primary: #615745;
    s-on-primary-fixed: #4F4638;
    s-on-primary-fixed-variant: #736752;
    s-primary-container: #736752;
    s-on-primary-container: #B1A691;
    
    s-secondary: rgb(164, 151, 127);
    s-secondary-fixed: rgb(177, 166, 145);
    s-secondary-fixed-dim: rgb(164, 151, 127);
    s-on-secondary: rgb(97, 87, 69);
    s-on-secondary-fixed: rgb(79, 70, 56);
    s-on-secondary-fixed-variant: rgb(115, 103, 82);
    s-secondary-container: rgb(115, 103, 82);
    s-on-secondary-container: rgb(177, 166, 145);
    
    s-tertiary: #BEB4A4;
    s-tertiary-fixed: #BEB4A4;
    s-tertiary-fixed-dim: #BEB4A4;
    s-on-tertiary: #2A261E;
    s-on-tertiary-fixed: #3C362B;
    s-on-tertiary-fixed-variant: #85785F;
    s-tertiary-container: #85785F;
    s-on-tertiary-container: #BEB4A4;
    
    s-error: #E5E1DA;
    s-on-error: #CBC3B6;
    s-error-container: #D8D2C8;
    s-on-error-container: #F2F0ED;
    
    s-surface: #181511;
    s-on-surface: #F2F0ED;
    s-on-surface-variant: #CBC3B6;
    s-outline: #97886D;
    s-outline-variant: #85785F;
    s-shadow: #181511;
    s-scrim: #181511;
    s-inverse-surface: #F2F0ED;
    s-inverse-on-surface: #3C362B;
    s-inverse-primary: #736752;
    s-surface-dim: #181511;
    s-surface-bright: #3C362B;
    s-surface-container-lowest: #181511;
    s-surface-container-low: #181511;
    s-surface-container: #2A261E;
    s-surface-container-high: #3C362B;
    s-surface-container-highest: #4F4638;

