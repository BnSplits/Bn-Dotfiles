    s-primary-container: #B21316;
    s-on-primary-container: #ED5659;
    
    s-secondary: rgb(234, 57, 61);
    s-secondary-fixed: rgb(237, 86, 89);
    s-secondary-fixed-dim: rgb(234, 57, 61);
    s-on-secondary: rgb(150, 16, 19);
    s-on-secondary-fixed: rgb(122, 13, 15);
    s-on-secondary-fixed-variant: rgb(178, 19, 22);
    s-secondary-container: rgb(178, 19, 22);
    s-on-secondary-container: rgb(237, 86, 89);
    
    s-tertiary: #F07274;
    s-tertiary-fixed: #F07274;
    s-tertiary-fixed-dim: #F07274;
    s-on-tertiary: #410708;
    s-on-tertiary-fixed: #5D0A0C;
    s-on-tertiary-fixed-variant: #CE161A;
    s-tertiary-container: #CE161A;
    s-on-tertiary-container: #F07274;
    
    s-error: #F9C7C8;
    s-on-error: #F38E90;
    s-error-container: #F6AAAC;
    s-on-error-container: #FCE3E3;
    
    s-surface: #250405;
